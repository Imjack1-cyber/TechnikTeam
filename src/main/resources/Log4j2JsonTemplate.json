{
	"timestamp": {
		"$resolver": "timestamp",
		"pattern": {
			"format": "yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
			"timeZone": "Europe/Berlin"
		}
	},
	"level": {
		"$resolver": "level",
		"field": "name"
	},
	"loggerName": {
		"$resolver": "logger",
		"field": "name"
	},
	"threadName": {
		"$resolver": "thread",
		"field": "name"
	},
	"message": {
		"$resolver": "message",
		"stringified": true
	}
}